pipeline {
    agent any

    tools{
      python 'python3'
    }

    stages {
    
      stage('Cheking') {

        steps {

            git branch:'main',url: 'https://github.com/br-chandan/Devops-Lab-6/'

        }
        
      }

      
      stage('Testing') {

            steps {
                echo 'Testing'
                sh 'python3 Hello.py'
            }
            
      }

        
    }
            post {

            success {
                echo 'Completed Execution'
            }
            failure {
                echo 'Execution  Failed'
            }

        }
    
}
